            android:label="@string/title_activity_add_recipe_scrolling"
            android:theme="@style/Theme.8477_Daniella_Queiroz_AndroidPrototype.NoActionBar" />
        <activity android:name=".service.loginMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".service.MainActivity"
            android:label="@string/title_activity_login_scrolling">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>